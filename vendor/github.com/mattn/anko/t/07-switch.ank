x = 0
r = -1
switch x {
case 0:
 r = 0
case 1:
 r = 1
case 2:
 r = 2
}
is(0, r, "switch/case")

x = 3
r = -1
switch x {
case 0:
 r = 0
case 1:
 r = 1
case 2:
 r = 2
}
is(-1, r, "switch/case")

x = 3
r = -1
switch x {
case 0:
 r = 0
case 1:
 r = 1
case 2:
 r = 2
default:
 r = 3
}
is(3, r, "switch/default")

