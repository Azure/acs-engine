    "apiVersion": "[variables('apiVersionLinkDefault')]",
    "dependsOn": [
        "[concat('EXTENSION_TARGET_VM_NAME_PREFIX', copyIndex(), '/cse', copyIndex())]"
    ],
    "copy": {
        "count": "EXTENSION_LOOP_COUNT",
        "name": "helloWorldExtensionLoop"
    },
    "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri": "EXTENSION_URL_REPLACEextensions/hello-world-k8s/v1/template.json",
            "contentVersion": "1.0.0.0"
        },
        "parameters": {
            "artifactsLocation": {
                "value": "EXTENSION_URL_REPLACE"
            },
            "apiVersionCompute": {
                "value": "[variables('apiVersionDefault')]"
            },
            "targetVMName": {
                "value": "[concat('EXTENSION_TARGET_VM_NAME_PREFIX', copyIndex())]"
            },
            "extensionParameters": {
                "value": "EXTENSION_PARAMETERS_REPLACE"
            }
        }
    }
}
