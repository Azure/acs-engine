{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Swarm"
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "masterdns1",
      "vmSize": "Standard_D2_v2"
    },
    "agentPoolProfiles": [
      {
        "name": "publicwin1",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "dnsPrefix": "publicwin1",
        "osType": "Windows",
        "ports": [
          80,
          443,
          8080
        ]
      },
      {
        "name": "privatewin",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "osType": "Windows"
      }
    ],
    "windowsProfile": {
      "adminUsername": "azureuser",
      "adminPassword": "replacepassword1234$"
    },
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "ssh-rsa PUBLICKEY azureuser@linuxvm"
          }
        ]
      }
    }
  }
}